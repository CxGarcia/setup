    // Statusbar
    "statusBar.background": "#333333",
    "statusBar.noFolderBackground": "#333333",

    // Title bar (macOS
    // "titleBar.activeBackground": "#333333",
    // "titleBar.activeForeground": "#eff0eb",
    // "titleBar.inactiveBackground": "#333333",
    // "titleBar.inactiveForeground": "#eff0eb",

    // Notifications
    // "notification.infoBackground": "#9aedfe",
    // "notification.infoForeground": "#282a36",
    // "notification.warningBackground": "#fff36b",
    // "notification.warningForeground": "#282a36",
    // "notification.errorBackground": "#ff5c57",
    // "notification.errorForeground": "#282a36",



    // Git decorations
    "gitDecoration.modifiedResourceForeground": "#57c7ff",
    "gitDecoration.deletedResourceForeground": "#ff5c57",
    "gitDecoration.untrackedResourceForeground": "#5af78e",
    "gitDecoration.ignoredResourceForeground": "#616263",
    "gitDecoration.conflictingResourceForeground": "#ff6ac1"
  }
}
